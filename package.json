{
  "name": "e-liquid-calculator-app",
  "productName": "e-Liquid Calculator",
  "description": "Application for calculating e-Liquids",
  "author": "Mike Smith",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-scripts": "2.1.1"
  },
  "main": "src/electron-starter.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "dev": "nf start",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "install-app-deps"
  },
  "build": {
    "appId": "com.electron.e-liquid-calculator-app",
    "win": {
      "target": "portable"
    },
    "directories": {
      "buildResources": "public"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "electron": "^3.0.9",
    "foreman": "^3.0.1",
    "electron-builder": "^20.34.0"
  },
  "homepage": "./"
}
