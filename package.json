{
  "name": "e-liquid-calculator-app",
  "productName": "e-Liquid Calculator",
  "description": "Application for calculating e-Liquids",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-scripts": "2.1.1"
  },
  "main": "src/electron-starter.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "dev": "nf start",
    "package-win": "electron-packager . --overwrite --asar --platform=win32 --arch=ia32 --icon=src/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"e-Liquid Calculator\"",
    "create-installer-win": "node installers/windows/createinstaller.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "electron": "^3.0.9",
    "electron-packager": "^12.2.0",
    "electron-winstaller": "^2.7.0",
    "foreman": "^3.0.1"
  },
  "homepage": "./"
}
